<?xml version="1.0" encoding="UTF-8"?>
<nomenclatures xmlns="http://www.ekylibre.org/XML/2013/nomenclatures">
  <nomenclature name="document_natures">
    <taggers>
      <tagger name="production_natures" prefix="p" file="production-natures.xml"/>
      <tagger name="management_aspects" prefix="m" file="management-aspects.xml"/>
    </taggers>
    <attributes>
      <attribute name="datasource" type="string"/>
    </attributes>

    <items>
      <item name="sales_order" tags="m:sales"/>
      <item name="sales_estimate" tags="m:sales"/>
      <item name="sales_invoice" tags="m:sales"/>
      <item name="purchase_order" tags="m:purchases"/>
      <item name="purchase_estimate" tags="m:purchases"/>
      <item name="purchase_invoice" tags="m:purchases"/>

      <item name="deposit" tags="m:finances"/>

      <item name="balance_sheet" tags="m:accountancy"/>
      <item name="income_statement" tags="m:accountancy"/>
      <item name="journal" tags="m:accountancy"/>
      <item name="general_journal" tags="m:accountancy"/>
      <item name="general_ledger" tags="m:accountancy"/>

      <item name="inventory" tags="m:stocks"/>
      <item name="stocks" tags="m:stocks"/>
      <item name="incoming_delivery_docket" tags="m:stocks, m:identification"/>
      <item name="outgoing_delivery_docket" tags="m:stocks, m:identification"/>

      <item name="animal_sheet" tags="m:production, p:animal_farming"/>

      <item name="entity_sheet" tags="m:relationship"/>

    </items>
  </nomenclature>
  
</nomenclatures>
