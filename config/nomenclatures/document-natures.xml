<?xml version="1.0" encoding="UTF-8"?>
<nomenclatures xmlns="http://www.ekylibre.org/XML/2013/nomenclatures">
  <nomenclature name="document-natures">
    <taggers>
      <tagger name="production-natures" prefix="p" file="production-natures.xml"/>
      <tagger name="management-aspects" prefix="m" file="management-aspects.xml"/>
    </taggers>
    <attributes>
      <attribute name="datasource" type="string"/>
    </attributes>

    <items>
      <item name="sales-order" tags="m:sales">
	<attribute name="datasource" value="sales-order"/>
      </item>
      <item name="sales-estimate" tags="m:sales">
	<attribute name="datasource" value="sales-estimate"/>
      </item>
      <item name="sales-invoice" tags="m:sales">
	<attribute name="datasource" value="sales-invoice"/>
      </item>

      <item name="purchase-order" tags="m:purchases">
	<attribute name="datasource" value="purchase-order"/>
      </item>
      <item name="purchase-estimate" tags="m:purchases">
	<attribute name="datasource" value="purchase-estimate"/>
      </item>
      <item name="purchase-invoice" tags="m:purchases">
	<attribute name="datasource" value="purchase-invoice"/>
      </item>

      <item name="deposit" tags="m:finances">
	<attribute name="datasource" value="deposit"/>
      </item>

      <item name="balance-sheet" tags="m:accountancy">
	<attribute name="datasource" value="trial-balance"/>
      </item>
      <item name="income-statement" tags="m:accountancy">
	<attribute name="datasource" value="trial-balance"/>
      </item>
      <item name="journal" tags="m:accountancy">
	<attribute name="datasource" value="full-journal"/>
      </item>
      <item name="general-journal" tags="m:accountancy">
	<attribute name="datasource" value="general-journal"/>
      </item>
      <item name="general-ledger" tags="m:accountancy">
	<attribute name="datasource" value="general-ledger"/>
      </item>

      <item name="inventory" tags="m:stocks">
	<attribute name="datasource" value="products"/>
      </item>
      <item name="stocks" tags="m:stocks">
	<attribute name="datasource" value="products"/>
      </item>
      <item name="incoming-delivery-docket" tags="m:stocks, m:identification">
	<attribute name="datasource" value="incoming-delivery"/>
      </item>
      <item name="outgoing-delivery-docket" tags="m:stocks, m:identification">
	<attribute name="datasource" value="outgoing-delivery"/>
      </item>

      <item name="animal-sheet" tags="m:production, p:animal-farming">
	<attribute name="datasource" value="animal"/>
      </item>

      <item name="entity-sheet" tags="m:relationship">
	<attribute name="datasource" value="entity"/>
      </item>

    </items>
  </nomenclature>
  
</nomenclatures>
