<?xml version="1.0" encoding="UTF-8"?>
<nomenclatures xmlns="http://www.ekylibre.org/XML/2013/nomenclatures">

  <nomenclature name="procedure_natures" hierarchical="true">
    <property-natures>
      <property-nature name="roles" type="list" nomenclature="procedure_roles" required="false"/>
    </property-natures>
    <items>
      <item name="animal_feeding" roles="input, target, doer, tool"/>
      <item name="animal_health" roles="input, target, doer"/>
      <item name="animal_fecondation" roles="input_origin, input, target, doer"/>
      <item name="animal_moving" roles="input, target, doer"/>
      <item name="animal_group_changing" roles="input, target"/>
      <item name="animal_production" roles="target, output, doer"/>
      <item name="viviparous_birth" roles="target, output"/>
      <item name="oviparous_birth" roles="target, output"/>
      <item name="harvest" roles="target, output, doer, tool"/>
      <item name="haymaking" roles="target, output, doer, tool"/>
      <item name="transformation" roles="input, output, doer, tool"/>
      <item name="transport" roles="input, doer, input_origin, tool, target"/>
      <item name="tillage" roles="target"/>
      <item name="plant_nutrition" roles="input, target"/>
      <item name="plant_illness_treatment" roles="input, target, doer, tool"/>
      <item name="soil_enrichment" roles="input, target, tool, doer, input_origin"/>
      <item name="soil_cleaning" roles="target"/>
      <item name="implantation" roles="input, target, output, doer, tool"/>
      <item name="pruning" roles="doer, target"/>
      <item name="grinding" roles="input, output, doer, tool"/>
      <item name="growth_control" roles="target"/>
      <item name="maintenance" roles="target"/>
    </items>
  </nomenclature>

  <nomenclature name="procedure_natures-implantation">
    <items>
      <item name="sowing" roles="input, target, output, doer, tool"/>
      <item name="implanting" roles="input, target, output, doer, tool"/>
    </items>
  </nomenclature>

  <nomenclature name="procedure_natures-maintenance">
    <items>
      <item name="consummables_addition" roles="target, doer, input"/>
      <item name="piece_replacement" roles="target, doer, input"/>
    </items>
  </nomenclature>
  
  <nomenclature name="procedure_natures-transformation">
    <items>
      <item name="transfer"     roles="input, target, doer"/>
      <item name="fermentation" roles="input, target, output, doer, tool"/>
      <item name="pressing"     roles="input, target, output, doer, tool"/>
      <item name="blending"     roles="input, target, output, doer"/>
      <item name="addition"     roles="input, target, doer"/>
      <item name="filtration"   roles="input, target, output, doer, tool"/>
      <item name="bottling"     roles="target, input, output, doer, tool"/>
      <item name="mixing"       roles="input, output, doer"/>
      <item name="remontage"    roles="target, doer"/>
    </items>
  </nomenclature>
  
  <nomenclature name="procedure_natures-addition">
    <!-- <items> -->
    <!--   <item name="wine_addition" roles="input, target, doer"/> -->
    <!-- </items> -->
  </nomenclature>
  
  <nomenclature name="procedure_natures-tillage">
    <items>
      <item name="plowing" roles="target, doer"/>
      <item name="raking" roles="target, doer"/>
      <item name="hoeing" roles="target"/>
      <item name="cultipacking" roles="target"/>
    </items>
  </nomenclature>

  <nomenclature name="procedure_natures-animal_health">
    <items>
      <item name="animal_illness_treatment" roles="input, target, doer"/>
      <item name="animal_housing_treatment" roles="input, target, doer, tool"/>
      <item name="animal_housing_cleaning" roles="output, target, doer, tool"/>
    </items>
  </nomenclature>
  
  <nomenclature name="procedure_natures-animal_illness_treatment">
    <items>
      <item name="animal_curative_medicine_admission" roles="input, target, doer"/>
      <item name="animal_preventive_medicine_admission" roles="input, target, doer"/>
    </items>
  </nomenclature>
  
  <nomenclature name="procedure_natures-animal_production">
    <items>
      <item name="milk_production" roles="output, target, doer"/>
    </items>
  </nomenclature>
  
  <nomenclature name="procedure_natures-animal_feeding">
    <items>
      <item name="food_supply" roles="input, target, doer, tool"/>
      <item name="pasturing" roles="input, target, doer"/>
    </items>
  </nomenclature>

  <nomenclature name="procedure_natures-plant_nutrition">
    <items>
      <item name="plant_irrigation" roles="input, target"/>
      <item name="plant_fertilisation" roles="input, target, doer"/>
    </items>
  </nomenclature>

</nomenclatures>
