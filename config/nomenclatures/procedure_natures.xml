<?xml version="1.0" encoding="UTF-8"?>
<nomenclatures xmlns="http://www.ekylibre.org/XML/2013/nomenclatures">

  <nomenclature name="procedure_natures" hierarchical="true">
    <attributes>
      <attribute name="roles" type="list" choices="input, output, target, doer" required="false"/>
    </attributes>
    <items>
      <item name="animal_feeding" roles="input, target, doer"/>
      <item name="animal_illness_treatment" roles="input, target, doer"/>
      <item name="animal_production" roles="target, output"/>
      <item name="viviparous_birth" roles="target, output"/>
      <item name="oviparous_birth" roles="target, output"/>
      <item name="harvest" roles="target, output"/>
      <item name="transformation" roles="input, output"/>
      <item name="tillage" roles="target"/>
      <item name="plant_fertilisation" roles="input, target"/>
      <item name="plant_illness_treatment" roles="input, target"/>
      <item name="soil_enrichment" roles="input, target"/>
      <item name="soil_cleaning" roles="target"/>
      <item name="sowing" roles="input, target, output"/>
      <item name="pruning" roles="target"/>
      <item name="grinding" roles="input, output"/>
      <item name="growth_control" roles="target"/>
    </items>
  </nomenclature>

  <nomenclature name="procedure_natures-tillage">
