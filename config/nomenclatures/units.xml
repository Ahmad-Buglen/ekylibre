<?xml version="1.0" encoding="UTF-8"?>
<nomenclatures xmlns="http://www.ekylibre.org/XML/2013/nomenclatures">
  <nomenclature name="units">
    <attributes>
      <attribute name="dimension" type="choice" nomenclature="dimensions" required="true"/>
      <attribute name="symbol" type="string" required="true"/>
      <attribute name="coefficient" type="decimal" required="true" default="1"/>
      <attribute name="coefficients" type="string"/>
      <attribute name="start" type="decimal" required="true" default="0"/>
      <attribute name="a" type="decimal" default="1"/>
      <attribute name="b" type="decimal" default="0"/>
    </attributes>
    <items>
      <!-- none -->
      <item name="percent"  dimension="none" symbol="%"   coefficients="0.01" a="0.01"/>
      <item name="unity"    dimension="none" symbol="u"   coefficients="1" a="1"/>
      <item name="pair"     dimension="none" symbol="Ⅱ"  coefficients="2" a="2"/>
      <item name="ten"      dimension="none" symbol="Ⅹ"   coefficients="10" a="10"/>
      <item name="dozen"    dimension="none" symbol="Ⅻ" coefficients="12"  a="12"/>
      <item name="hundred"  dimension="none" symbol="Ⅽ"  coefficients="100"     a="100"/>
      <item name="thousand" dimension="none" symbol="Ⅿ"  coefficients="1000"    a="1000"/>
      <item name="million"  dimension="none" symbol="ⅯⅯ" coefficients="1000000" a="1000000"/>
      
      <!-- temperature -->
      <item name="kelvin"    dimension="temperature" symbol="K"                  coefficients="1K"   a="1"/>
      <item name="celsius"   dimension="temperature" symbol="°C" start="-273.15" coefficients="1K"   a="1"   b="-273.15"/>
      <item name="farenheit" dimension="temperature" symbol="°F" start="-459.67" coefficients="1.8K" a="1.8" b="-459.67"/>

      <!-- mass -->
      <item name="microgram" dimension="mass" symbol="µg" coefficients="0.000000001kg" a="0.000000001"/>
      <item name="milligram" dimension="mass" symbol="mg" coefficients="0.000001kg"    a="0.000001"/>
      <item name="gram"      dimension="mass" symbol="g"  coefficients="0.001kg"       a="0.001"/>
      <item name="kilogram"  dimension="mass" symbol="kg" coefficients="1kg"           a="1"/>
      <item name="quintal"   dimension="mass" symbol="qt" coefficients="100kg"         a="100"/>
      <item name="ton"       dimension="mass" symbol="t"  coefficients="1000kg"        a="1000"/>

      <!-- length -->
      <item name="millimeter" dimension="length" symbol="mm" coefficients="0.001m" a="0.001"/>
      <item name="meter"      dimension="length" symbol="m"  coefficients="1m"     a="1"/>
      <item name="kilometer"  dimension="length" symbol="km" coefficients="1000m"  a="1000"/>

      <!-- surface_area -->
      <item name="square_meter" dimension="surface_area" symbol="m²"   coefficients="1m2"     a="1"/>
      <item name="are"          dimension="surface_area" symbol="a"    coefficients="100m2"   a="100"/>
      <!-- <item name="acre"         dimension="surface_area" symbol="acre" coefficients="4046.8564224m2"   a="4046.8564224"/> -->
      <item name="hectare"      dimension="surface_area" symbol="ha"   coefficients="10000m2" a="10000"/>

      <!-- volume -->
      <item name="cubic_meter" dimension="volume" symbol="m³" coefficients="1m3"        a="1"/>
      <item name="hectoliter"  dimension="volume" symbol="hl" coefficients="0.1m3"      a="0.1"/>
      <item name="liter"       dimension="volume" symbol="l"  coefficients="0.001m3"    a="0.001"/>
      <item name="milliliter"  dimension="volume" symbol="ml" coefficients="0.000001m3" a="0.000001"/>

      <!-- time -->
      <item name="second" dimension="time" symbol="s"   coefficients="1s"      a="1"/>
      <item name="minute" dimension="time" symbol="min" coefficients="60s"     a="60"/>
      <item name="hour"   dimension="time" symbol="h"   coefficients="3600s"   a="3600"/>
      <item name="day"    dimension="time" symbol="d"   coefficients="86400s"  a="86400"/>

      <!-- electric_current -->
      <item name="ampere"  dimension="electric_current" symbol="A" coefficients="A" a="1"/>    
      
      <!-- amount_of_substance -->
      <item name="mole"    dimension="amount_of_substance" symbol="mol" coefficients="mol" a="1"/>
      
      <!-- luminous_intensity -->
      <item name="candela" dimension="luminous_intensity" symbol="cd" coefficients="cd" a="1"/>

      <!-- mass_concentration -->
      <item name="microgram_per_liter" dimension="mass_concentration" symbol="µg/l" coefficients="1000000kg*0.001-1*m-3" a="1000000"/>
      <item name="milligram_per_liter" dimension="mass_concentration" symbol="mg/l" coefficients="0.000001kg*0.001m-3"   a="1000"/>
      <item name="gram_per_liter"      dimension="mass_concentration" symbol="g/l"  coefficients="0.001kg*0.001m-3"      a="1"/>
      <item name="kilogram_per_liter"  dimension="mass_concentration" symbol="kg/l" coefficients="kg*0.001m-3"           a="0.001"/>
      
      <!-- mass_area_density -->
      <item name="kilogram_per_square_meter"  dimension="mass_area_density" symbol="kg/m2" coefficients="kg*m-2"           a="1"/>
      <item name="liter_per_square_meter"  dimension="mass_area_density" symbol="l/m2" coefficients="l*m-2"           a="1"/>
      <item name="kilogram_per_hectare"  dimension="mass_area_density" symbol="kg/ha" coefficients="kg*10000m-2"           a="0.0001"/>
      <item name="quintal_per_hectare"  dimension="mass_area_density" symbol="qt/ha" coefficients="100kg*10000m-2"           a="0.01"/>
      

      <!-- concentration -->
      <item name="unity_per_liter"         dimension="concentration" symbol="u/l"   coefficients="0.001m-3"              a="0.001"/>
      <item name="thousand_per_liter"      dimension="concentration" symbol="ku/l"  coefficients="1000*0.001m-3"         a="0.000001"/>
      <item name="thousand_per_milliliter" dimension="concentration" symbol="ku/ml" coefficients="1000*0.000001m-3"      a="0.000000001"/>


      <item name="mass_percent" dimension="none" symbol="%kg"   coefficients="0.01" a="0.01"/>

      
      <!-- mass_density -->
      <item name="unity_per_kilogram"     dimension="mass_density" symbol="u/kg"  coefficients="kg-1"                    a="1"/>
      <!-- <item name="kilogram_per_hundred_kilogram"     dimension="mass_density" symbol="kg/100kg"  coefficients="100kg-1"                    a="0.01"/>      -->
      <item name="thousand_per_kilogram"  dimension="mass_density" symbol="ku/kg" coefficients="1000*kg-1"               a="0.001"/>
      <item name="thousand_per_hectogram" dimension="mass_density" symbol="ku/hg" coefficients="1000*0.1kg-1"            a="0.0001"/>
      <!-- <item name="kilogram_per_hectogram" dimension="mass_density" symbol="kg/100kg" coefficients="1*0.1kg-1"            a="0.0001"/> -->
      <!-- <item name="thousand_eq_per_hundred_gram" dimension="mass_density" symbol="meq/100g" coefficients="1000*0.1kg-1"/> -->
      
      <!-- power -->
      <item name="french_horsepower"  dimension="power" symbol="ch" a="735.5"/>
      <item name="english_horsepower" dimension="power" symbol="hp" a="745.6998"/>
      <item name="watt"     dimension="power" symbol="W" a="1"/>
      <item name="kilowatt" dimension="power" symbol="kW" a="1000"/>
      <item name="megawatt" dimension="power" symbol="MW" a="1000000"/>
      
      <!-- energy -->
      <item name="joule"  dimension="energy" symbol="J" a="1"/>
      <item name="kilowatt_hour"  dimension="energy" symbol="kWh" a="3600000"/>
      
    </items>
  </nomenclature>
</nomenclatures>
