<?xml version="1.0" encoding="UTF-8"?>
<nomenclatures xmlns="http://www.ekylibre.org/XML/2013/nomenclatures">
  <!-- root -->
  <nomenclature name="varieties-root">
    <attributes>
      <!-- <attribute name="derivatives" type="list" required="false" inherit="true" merge="true"/> -->
      <attribute name="derivative_of" type="choice" required="false" nomenclature="varieties" inherit="true"/>
    </attributes>
    <items>
      <item name="product"/>
    </items>
  </nomenclature>
  <!-- level 1 -->
  <nomenclature name="varieties-product">
    <items>
      <item name="matter"/>
      <item name="immatter"/>
      <item name="product_group"/>
    </items>
  </nomenclature>
  <!-- level 2 - branch 1 -->
  <nomenclature name="varieties-matter">
    <items>
      <item name="bioproduct"/>
      <item name="equipment"/>
      <item name="mineral_matter"/>
      <item name="organic_matter"/>
      <item name="zone"/>
    </items>
  </nomenclature>
  <!-- level 2 - branch 2 -->
  <nomenclature name="varieties-immatter">
    <items>
      <item name="service"/>
      <item name="property_title"/>
    </items>
  </nomenclature>
  <!-- level 2 - branch 3 -->
  <nomenclature name="varieties-product_group">
    <items>
      <item name="animal_group" derivative_of="animal"/>
      <item name="land_parcel_group" derivative_of="land_parcel"/>
      <item name="building"/>
    </items>
  </nomenclature>
  <!-- level 2 - branch 1 - sub-branch 1 -->
  <nomenclature name="varieties-bioproduct">
    <attributes>
      <attribute name="itis-tsn" type="string"/>
    </attributes>
    <items>
      <item name="animal"/>
      <item name="plant"/>  
      <item name="fungus"/>
      <!-- 
	   <item name="bacteria"/>
	   <item name="chromista"/>
	   <item name="protozoa"/>
      -->
    </items>
  </nomenclature>
  <!-- level 2 - branch 1 - sub-branch 1 -->
  <nomenclature name="varieties-organic_matter">
    <items>
      <item name="phytosanitary"/>
    </items>
  </nomenclature>
  <!-- level 2 - branch 1 - sub-branch 2 -->
  <nomenclature name="varieties-equipment">
    <items>
      <item name="worker"/>
    </items>
  </nomenclature>
  <!-- level 2 - branch 1 - sub-branch 3 -->
  <nomenclature name="varieties-zone">
    <items>
      <item name="easement"/>
      <item name="sub_zone"/>
    </items>
  </nomenclature>
  <!-- level 2 - branch 2 - sub-branch 1-->
  <nomenclature name="varieties-property_title">
    <items>
      <item name="zone_property_title"/>
    </items>
  </nomenclature> 
   <!-- level 2 - branch 3 - sub-branch 1-->
  <nomenclature name="varieties-land_parcel_group">
    <items>
      <item name="land_parcel_cluster"/>
    </items>
  </nomenclature>   
  <!-- level 2 - branch 1 - sub-branch 3 - leaf 1 -->
  <nomenclature name="varieties-easement">
    <items>
      <item name="settlement"/>
      <item name="land_parcel"/>
    </items>
  </nomenclature>
  <!-- level 2 - branch 1 - sub-branch 3 - leaf 2 -->
  <nomenclature name="varieties-sub_zone">
    <items>
      <item name="building_room"/>
    </items>
  </nomenclature>
</nomenclatures>
