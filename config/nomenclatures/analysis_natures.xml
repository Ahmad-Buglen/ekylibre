<?xml version="1.0" encoding="UTF-8"?>
<nomenclatures xmlns="http://www.ekylibre.org/XML/2013/nomenclatures">
  <!-- root -->
  <nomenclature name="analysis_natures">
    <attributes>
      <!-- Inside the model-->
      <attribute name="target_variety" type="choice" nomenclature="varieties" required="true"/>
      <attribute name="target_derivative_of" type="choice" nomenclature="varieties" required="false"/>
      <attribute name="items" type="list" nomenclature="indicators" required="true"/>
    </attributes>
    <items>
      <item name="cow_milk_analysis"
            target_variety="milk"
            target_derivative_of="bos"
            items="total_bacteria_concentration, inhibitors_presence, fat_matters_concentration, protein_matters_concentration, somatic_cell_concentration, clostridial_spores_concentration, freezing_point_temperature, lipolysis, immunoglobulins_concentration, urea_concentration"
            />
      <item name="unitary_cow_milk_analysis"
            target_variety="milk"
            target_derivative_of="bos"
            items="fat_matters_concentration, protein_matters_concentration, somatic_cell_concentration"
            />
    </items>

  </nomenclature>

</nomenclatures>
