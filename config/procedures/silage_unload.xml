<?xml version="1.0"?>
<procedures xmlns="http://www.ekylibre.org/XML/2013/procedures">
  <!-- ######################################## TRAITEMENT VETERINAIRE ########################################### -->
  <procedure name="silage_unload" natures="food_supply" version="0" status="frozen">
    <variables>
      <variable name="herd"             variety="animal_group" derivative_of="bos" roles="food_supply-target"/>
      <variable name="silage"           variety="preparation" abilities="feed(bos)"/>
      <variable name="silage_to_give" new="parted-from: silage" roles="food_supply-input" need="population"/>
      <variable name="caregiver"        variety="worker" roles="food_supply-doer"/>
      <variable name="tractor"          abilities="tow(equipment), move" roles="food_supply-tool"/>
      <variable name="silage_unload"    abilities="deliver(silage)" roles="food_supply-tool"/>
    </variables>
    <operations>
      <operation id="100">
        <task do="[caregiver] moves at [herd]"/>
      </operation>
      <operation id="200" duration="10 minutes">
        <task do="[silage] parts with [silage_to_give]"/>
        <task do="[herd] consumes [silage_to_give]"/>
      </operation>
    </operations>
  </procedure>

</procedures>
