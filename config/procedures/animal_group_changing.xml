<?xml version="1.0"?>
<procedures xmlns="http://www.ekylibre.org/XML/2013/procedures">
  <!-- ######################################## ANIMAUX ########################################### -->
  <procedure name="animal_group_changing" natures="animal_group_changing" version="0" status="frozen">
    <variables>
      <variable name="animal"             variety="animal" abilities="move" roles="animal_group_changing-input"/>
      <variable name="herd"               variety="animal_group" derivative-of="animal" roles="animal_group_changing-target" default-actor="storage"/>
    </variables>
    <operations>
      <operation id="100">
        <task do="[animal] goes into [herd]"/>
      </operation>
    </operations>
  </procedure>
</procedures>
