<?xml version="1.0"?>
<procedures xmlns="http://www.ekylibre.org/XML/2013/procedures">
  <procedure name="straw_bunching" categories="harvesting" actions="straw_bunching">
    <variables>
      <variable name="tractor" type="tool" variety="equipment"/>
      <variable name="baler_driver" type="doer" abilities="move, drive(tractor), drive(baler)"/>
      <variable name="baler" type="tool" variety="equipment" abilities="bunch"/>
      <variable name="straw_to_bunch" type="input" variety="straw"/>
      <variable name="straw_bales" type="output" default-name="{{variant}} [{{birth_day}}/{{birth_month}}/{{birth_year}}] ({{container}})">
        <handler indicator="population"/>
      </variable>
    </variables>
  </procedure>
</procedures>
