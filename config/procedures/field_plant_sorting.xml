<?xml version="1.0"?>
<procedures xmlns="http://www.ekylibre.org/XML/2013/procedures">
  <procedure name="field_plant_sorting" categories="crop_maintenance" actions="field_plant_sorting" deprecated="true">
    <parameters>
      <target name="sortable" filter="is seedling" default-name="{{variant}} [{{birth_day}}/{{birth_month}}/{{birth_year}}] ({{container}})"/>
      <doer name="doer"  filter="is worker and can move"/>
      <tool name="container" filter="can store(product)" default-actor="default_storage_of_support"/>
      <tool name="variant" filter="is seedling"/>
    </parameters>
  </procedure>
</procedures>