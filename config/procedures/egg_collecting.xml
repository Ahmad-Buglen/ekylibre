<?xml version="1.0"?>
<procedures xmlns="http://www.ekylibre.org/XML/2013/procedures">
  <procedure name="egg_collecting" categories="animal_production" actions="egg_collecting">
    <variables>
      <variable name="bird_band" type="target" variety="aves" abilities="produce(egg)"/>
      <variable name="eggs" type="output" variety="egg" derivative-of="variety:bird_band" default-name="{{variant}} [{{birth_day}}/{{birth_month}}/{{birth_year}}] ({{container}})">
        <handler indicator="population"/>
      </variable>
      <variable name="doer" type="doer" variety="worker"/>
    </variables>
  </procedure>
</procedures>
