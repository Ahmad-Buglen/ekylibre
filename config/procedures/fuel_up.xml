<?xml version="1.0"?>
<procedures xmlns="http://www.ekylibre.org/XML/2013/procedures">
  <procedure name="fuel_up" categories="energy_supplying" actions="fuel_up">
    <parameters>
      <parameter name="equipment" type="target" filter="is equipment and can consume(fuel)"/>
      <parameter name="fuel" type="input" filter="is fuel">
        <handler indicator="population"/>
      </parameter>
      <parameter name="mechanic" type="doer" filter="can drive(equipment)"/>
    </parameters>
  </procedure>
</procedures>