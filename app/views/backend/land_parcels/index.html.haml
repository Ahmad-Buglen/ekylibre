- main_period_selector(:year)

= janus do |j|
  - land_parcels_map(janus: j, optional_data_url: backend_visualizations_non_treatment_areas_visualizations_path)
  - j.face :list do
    = kujaku do |k|
      - k.text
    - main_toolbar do |t|
      .controls
        %button{class: "btn btn-default #{'disabled' unless (Activity.any? { |activity| activity.plant_farming? } && CultivableZone.any?) }", id: "generate-land-parcel-btn"}
          = :generate_land_parcel.tl

    = main_list
  