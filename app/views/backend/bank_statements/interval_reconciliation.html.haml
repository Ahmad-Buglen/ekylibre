= form_tag(backend_bank_reconciliation_items_path, method: :get) do
  = hidden_field_tag :cash_id, params[:cash_id]
  = field_set :interval do
    = field "débuté le", date_field_tag(:period_start)
    = field "stoppé le", date_field_tag(:period_end)
  = form_actions do
    = submit_tag :validate.tl
