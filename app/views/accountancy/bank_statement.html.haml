=toolbar do |t|
  -t.update @bank_statement
  -t.point @bank_statement

=attributes_list(@bank_statement) do |l|
  -l.attribute(:number)
  -l.attribute(:cash, :url=>{:action=>:cash, :controller=>:finances})
  -l.attribute(:credit)
  -l.attribute(:debit)
  -l.attribute(:started_on)
  -l.attribute(:stopped_on)

%h2=BankStatement.human_attribute_name("lines")
=kame(:bank_statement_lines)

